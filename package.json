  "description": "Personal Knowledge Management Application",
  "main": "./dist/main/main.js",
  "scripts": {
    "clean": "node clean.js",
    "start": "electron .",
    "dev": "concurrently \"npm run watch-main\" \"npm run watch-renderer\" \"npm run electron\"",
    "electron": "wait-on dist/main/main.js && wait-on dist/renderer/renderer.js && electron .",
    "watch-main": "tsc -w -p tsconfig.main.json",
    "watch-renderer": "webpack --watch",
    "build": "node build.js",
    "test": "jest"
  },
